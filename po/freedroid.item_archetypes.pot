#: ./freedroid.item_archetypes:3840
#: ./freedroid.item_archetypes:3873
#: ./freedroid.item_archetypes:3906
#: ./freedroid.item_archetypes:3939
#: ./freedroid.item_archetypes:3972
#: ./freedroid.item_archetypes:4005
#: ./freedroid.item_archetypes:4038
#: ./freedroid.item_archetypes:4071
#: ./freedroid.item_archetypes:4104
#: ./freedroid.item_archetypes:4137
#: ./freedroid.item_archetypes:4170
#: ./freedroid.item_archetypes:4203
#: ./freedroid.item_archetypes:4236
#: ./freedroid.item_archetypes:4270
#: ./freedroid.item_archetypes:4305
#: ./freedroid.item_archetypes:4340
msgid "Droid weaponry"
msgstr ""

#: ./freedroid.item_archetypes:3827
msgid "Droid 139 Plasma Trash Vaporiser"
msgstr ""

#: ./freedroid.item_archetypes:3860
msgid "Droid 247 Robotic Arm"
msgstr ""

#: ./freedroid.item_archetypes:3893
msgid "Droid 249 Pulse Laser Welder"
