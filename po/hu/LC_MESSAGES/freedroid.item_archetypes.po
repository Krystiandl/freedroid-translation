msgstr "Droid fegyverzet - 139 :  Plazma szemétmegsemmisítő"

#: freedroid.item_archetypes:2240
msgid "Droid weaponry - 247 : Weak robotic arm"
msgstr "Droid fegyverzet - 247 : Gyenge robotkar"

#: freedroid.item_archetypes:2275
msgid "Droid weaponry - 249 : Pulse Laser welder"
msgstr "Droid fegyverzet - 249 : Impulzuslézer-hegesztő"

#: freedroid.item_archetypes:2308
msgid "Droid weaponry - 296 : Plasma beam cutter"
msgstr "Droid fegyverzet - 296 : Plazmasugár-vágó"

#: freedroid.item_archetypes:2343
msgid "Droid weaponry - 302"
msgstr "Droid fegyverzet - 302"

#: freedroid.item_archetypes:2378
