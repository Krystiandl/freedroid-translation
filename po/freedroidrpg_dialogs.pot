msgid "Welcome to MS firmware upgrade server for region #54658."
msgstr ""

#: ../dialogs/FirmwareUpdateServer.dialog:34
msgid "Please select action"
msgstr ""

#: ../dialogs/FirmwareUpdateServer.dialog:39
msgid "Get current OS version"
msgstr ""

#: ../dialogs/FirmwareUpdateServer.dialog:42
msgid "Currently deployed firmware is v. 5.0.8+worldto-nogpl."
msgstr ""

#: ../dialogs/FirmwareUpdateServer.dialog:44
msgid "Currently deployed firmware is v. 5.0.8+tuxwashere+gpl"
msgstr ""

#: ../dialogs/FirmwareUpdateServer.dialog:50
msgid "Propagate firmware update"
msgstr ""

#: ../dialogs/FirmwareUpdateServer.dialog:52
msgid "Compressing image...."
msgstr ""

#: ../dialogs/FirmwareUpdateServer.dialog:53
msgid "Advertising update to clients..."
msgstr ""

#: ../dialogs/FirmwareUpdateServer.dialog:54
msgid "Waiting for all connections...."
msgstr ""

