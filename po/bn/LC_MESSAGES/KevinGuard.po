#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:109
#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:117
msgid "SEVEN."
msgstr ""

#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:110
#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:118
msgid "SIX."
msgstr ""

#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:115
msgid "I am not afraid of you."
msgstr ""

#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:125
msgid "FIVE."
msgstr ""

#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:126
msgid "FOUR."
msgstr ""

