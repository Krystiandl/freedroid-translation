#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:93
msgid "DOOR OPENING SEQUENCE INITIATED."
msgstr ""

#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:98
msgid "Step aside, you stupid tin can!"
msgstr ""

#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:100
msgid ""
"YOU SHALL NOT PASS. ACTIVATING DISINTEGRATOR BEAM IN TEN SECONDS."
msgstr ""

#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:101
#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:155
msgid "NINE."
msgstr ""

#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:102
#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:156
msgid "EIGHT."
msgstr ""

#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:107
#: /home/david/src/freedroid-translation/dialogs/KevinGuard.dialog:123
msgid "Wait, that's not what I meant!"
